{
  "name": "action_slack-notify",
  "version": "1.0.0",
  "description": "Slackへのワークフロー結果通知アクション",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "test-watch": "jest --watch",
    "ci-test": "jest --forceExit --detectOpenHandles",
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "tslint 'src/**/*.ts{,x}'",
    "pack": "tsc && ncc build",
    "all": "npm run lint && npm test && npm run pack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mm0202/action_slack-notify.git"
  },
  "keywords": [
    "slack",
    "github",
    "action",
    "github action"
  ],
  "author": "m",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mm0202/action_slack-notify/issues"
  },
  "homepage": "https://github.com/mm0202/action_slack-notify#readme",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^2.1.1",
    "@slack/webhook": "^5.0.3"
  },
  "devDependencies": {
    "@types/jest": "^24.9.1",
    "@zeit/ncc": "^0.21.1",
    "dotenv": "^8.2.0",
    "jest": "^24.9.0",
    "ts-jest": "^24.3.0",
    "tslint": "^6.1.0",
    "typescript": "^3.8.3"
  }
}
